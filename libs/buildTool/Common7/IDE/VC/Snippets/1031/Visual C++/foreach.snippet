<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>foreach</Title>
			<Shortcut>foreach</Shortcut>
			<Description>Codeausschnitt f√ºr foreach-Anweisung</Description>
			<Author>Microsoft Corporation</Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
				<SnippetType>SurroundsWith</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>type</ID>
					<ToolTip>Der Typ des Objekts in der Sammlung, die durchlaufen werden soll.</ToolTip>
					<Default>object</Default>
				</Literal>
				<Literal>
					<ID>indentifier</ID>
					<ToolTip>Ein Bezeichner, der das Element in der Sammlung darstellt.</ToolTip>
					<Default>var</Default>
				</Literal>
				<Literal>
					<ID>collection</ID>
					<ToolTip>Der Name der Sammlung oder des Arrays, die bzw. das durchlaufen werden soll.</ToolTip>
					<Default>collection_to_loop</Default>
				</Literal>
			</Declarations>
			<Code Language="cpp"><![CDATA[for each ($type$ $indentifier$ in $collection$)
	{
		$selected$ $end$
	}]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>
