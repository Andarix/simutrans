              <xs:element name="IncludeAssets">
                <xs:annotation>
                  <xs:documentation>
                    <!-- _locID_text="DotNetCliToolReference_IncludeAssets" _locComment="" -->In diesen Verweis einzubeziehende Ressourcen
                    </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="ExcludeAssets">
                <xs:annotation>
                  <xs:documentation>
                    <!-- _locID_text="DotNetCliToolReference_ExcludeAssets" _locComment="" -->Von diesem Verweis auszuschließende Ressourcen
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="PrivateAssets">
                <xs:annotation>
                  <xs:documentation>
                    <!-- _locID_text="DotNetCliToolReference_PrivateAssets" _locComment="" -->Ressourcen, die in diesem Verweis privat sind
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:choice>
          </xs:sequence>
          <xs:attribute name="Include" type="xs:string">
            <xs:annotation>
              <xs:documentation>
                <!-- _locID_text="DotNetCliToolReference_Include" _locComment="" -->Paketname des Tools. Dieser weicht unter Umständen vom Namen des zugeordneten Referenzpakets ab.
 </xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="Version" type="xs:string" use="optional">
            <xs:annotation>
              <xs:documentation>
                <!-- _locID_text="DotNetCliToolReference_Version" _locComment="" -->Version der Abhängigkeit
              </xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="Xdcmake" substitutionGroup="msb:Item">
