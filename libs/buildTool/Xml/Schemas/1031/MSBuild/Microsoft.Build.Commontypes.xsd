    <xs:element name="AspNetConfiguration" type="msb:StringPropertyType" substitutionGroup="msb:Property"/>
    <xs:element name="AspNetCoreHostingModel" type="msb:StringPropertyType" substitutionGroup="msb:Property">
        <xs:annotation>
