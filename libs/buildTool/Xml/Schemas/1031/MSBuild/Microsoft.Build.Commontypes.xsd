                                    <xs:documentation><!-- _locID_text="PublishFile_IncludeHash" _locComment="" -->(boolesch)</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="IsAssembly" type="msb:boolean">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="PublishFile_IsAssembly" _locComment="" -->(boolesch)</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="PublishState">
                                <xs:annotation>
                                    <xs:documentation><!-- _locID_text="PublishFile_PublishState" _locComment="" -->"Default", "Included", "Excluded", "DataFile", "ManifestEntryPoint" oder "Prerequisite"</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:choice>
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="TargetPlatform" substitutionGroup="msb:Item">
        <xs:annotation>
            <xs:documentation>
                <!-- _locID_text="TargetPlatform" _locComment="" -->Zielplattform in der Form "[Bezeichner], Version=[Version]", z. B. "Windows, Version=8.0"
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="Analyzer" substitutionGroup="msb:Item">
