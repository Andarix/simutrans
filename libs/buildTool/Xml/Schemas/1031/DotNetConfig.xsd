                                                                                            </xs:complexType>
                                                                                        </xs:element>
                                                                                        <xs:element name="servicePrincipalName" vs:help="configuration/system.serviceModel/standardEndpoints/dynamicEndpoint/standardEndpoint/discoveryClientSettings/endpoint/identity/servicePrincipalName">
