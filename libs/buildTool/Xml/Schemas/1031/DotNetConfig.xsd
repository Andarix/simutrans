                                                                                                                    </xs:annotation>
                                                                                                                    <xs:restriction base="xs:string">
                                                                                                                        <xs:enumeration value="CurrentUser"/>
                                                                                                                        <xs:enumeration value="LocalMachine"/>
                                                                                                                    </xs:restriction>
                                                                                                                </xs:simpleType>
                                                                                                            </xs:attribute>
