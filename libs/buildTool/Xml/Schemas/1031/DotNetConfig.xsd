                                                                                                            <xs:enumeration value="FindByCertificatePolicy"/>
                                                                                                            <xs:enumeration value="FindByExtension"/>
                                                                                                            <xs:enumeration value="FindByKeyUsage"/>
                                                                                                            <xs:enumeration value="FindBySubjectKeyIdentifier"/>
                                                                                                        </xs:restriction>
                                                                                                    </xs:simpleType>
                                                                                                </xs:attribute>
